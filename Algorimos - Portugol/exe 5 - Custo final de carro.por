programa {
  funcao inicio() {

    real custo_de_fabrica, custo_final

    escreva("Digite o custo de fabrica de seu carro: ")
      leia(custo_de_fabrica)

      custo_final = custo_de_fabrica*0.45 + custo_de_fabrica*0.28 + custo_de_fabrica

      escreva("O custo final de seu carro é: ", custo_final)


  }
}
